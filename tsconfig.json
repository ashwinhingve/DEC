{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/page.tsx",
    "next-env.d.ts",
    "out/types/**/*.ts",
    ".next/types/**/*.ts"
, "app/contact/page.jsx", "lib/dbConnects.js", "app/api/contact/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
